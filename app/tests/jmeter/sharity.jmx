          <JSR223Assertion guiclass="TestBeanGUI" testclass="JSR223Assertion" testname="JSR223 Assertion" enabled="true">
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="script">import com.fasterxml.jackson.databind.ObjectMapper;
import com.fasterxml.jackson.databind.JsonNode;
import java.util.ArrayList;
import java.util.List;

//List&lt;JsonNode&gt; postData = new ArrayList&lt;JsonNode&gt;();

try {
    ObjectMapper mapper = new ObjectMapper();
    List &lt; JsonNode &gt; postData = Arrays.asList(mapper.readValue(vars.getObject(&quot;GET_DATA&quot;), JsonNode[].class));
    //System.out.print(postData);

    if (!postData.isEmpty()) {
        JsonNode element = mapper.readValue(vars.getObject(&quot;POST_DATA&quot;), JsonNode.class);
        
        String userId = element.get(&quot;_id&quot;).asText();
        String dob = element.get(&quot;dateOfBirth&quot;).asText();
        String email = element.get(&quot;email&quot;).asText();
        String fname = element.get(&quot;firstName&quot;).asText();
        String lname = element.get(&quot;lastName&quot;).asText();
        String panno = element.get(&quot;panNumber&quot;).asText();
        String phoneno = element.get(&quot;phone&quot;).asText();
        
	   vars.put(&quot;DATA_ID&quot;, userId);
        boolean exists = false;

        for (int j = 0; j &lt; postData.size(); j++) {
            JsonNode postdataElement = postData.get(j);

            if (postdataElement.get(&quot;_id&quot;).asText().equals(userId) &amp;&amp;
                postdataElement.get(&quot;dateOfBirth&quot;).asText().equals(dob) &amp;&amp;
                postdataElement.get(&quot;email&quot;).asText().equals(email) &amp;&amp;
                postdataElement.get(&quot;firstName&quot;).asText().equals(fname) &amp;&amp;
                postdataElement.get(&quot;lastName&quot;).asText().equals(lname) &amp;&amp;
                postdataElement.get(&quot;panNumber&quot;).asText().equals(panno) &amp;&amp;
                postdataElement.get(&quot;phone&quot;).asText().equals(phoneno)) {
                System.out.print(postdataElement);
                exists = true;
                break;
            }
        }

        if (!exists) {
            AssertionResult.setFailureMessage(&quot;User: &quot; + userId + &quot; with First Name: &quot; + fname + &quot; and Last Name: &quot; + lname + &quot; doesn&apos;t exist in data.&quot;);
            AssertionResult.setFailure(true);
        }

    }

} catch (Exception e) {
    AssertionResult.setFailureMessage(e.getMessage());
    AssertionResult.setFailure(true);
}</stringProp>
